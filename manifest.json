{
	"name": "Chess extension",
	"description": "Improve chess websites",
	"version": "0.1",
	"manifest_version": 3,
	"content_scripts": [
		{
			"matches": ["https://chesstempo.com/opening-training/*"],
			"js": [
				"ct-opening.js"
			]
		}
	],
	"web_accessible_resources": [
		{
			"resources": [
				"lichess-explorer.js",
				"lichess-explorer.css",
				"lichess.woff2"
			],
			"matches": ["https://chesstempo.com/*"]
		}
	]
}
